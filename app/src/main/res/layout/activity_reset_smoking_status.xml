<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.hrzhulocal.crushthecraveprototype2.ResetSmokingStatus"
    android:background="@drawable/bg">

    <ScrollView
        android:id="@+id/who_we_are_textviewLayoutAcrollView"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:scrollbars="none"
        >
    <LinearLayout
        android:id="@+id/who_we_are_textviewLayout"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:weightSum="1">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/kookop"
        android:textSize="15dp"
        android:id="@+id/textView43"
        android:textColor="#ffffffff"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

        android:orientation="vertical"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/textView43"
        android:background="#ffffffff"
        android:layout_above="@+id/imageButton17">

        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:hint="@string/poopk"
            android:textColorHint="#FFFFFFFF"
            android:ems="10"
            android:id="@+id/editText10"
            android:layout_below="@+id/textView56"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="44dp"
            android:textColor="#FFFFFFFF" />

        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="10"
            android:id="@+id/editText11"
            android:textColor="#FFFFFFFF"
            android:hint="@string/ijkk"
            android:textColorHint="#FFFFFFFF"
            />

        <Spinner
            android:layout_width="165dp"
            android:layout_height="wrap_content"
            android:id="@+id/spinner7"
            android:background="@drawable/btnsighup_new"
            android:entries="@array/gender"
            android:prompt="@string/gender_prompt"
            android:layout_gravity="right" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="@string/lkjmilojm"
            android:id="@+id/textView45"
            android:textColor="#ffffffff" />

        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="number"
            android:ems="10"
            android:id="@+id/editText5"
            android:layout_gravity="right"
            android:editable="true"
            android:textColor="#ffffffff" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="@string/ojnoij"
            android:id="@+id/textView46"
            android:textColor="#ffffffff" />

        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="number"
            android:ems="10"
            android:id="@+id/editText7"
            android:textColor="#ffffffff" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="@string/nmbmn"
            android:id="@+id/textView47"
            android:textColor="#ffffffff" />

        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="number"
            android:ems="10"
            android:id="@+id/editText8"
            android:textColor="#ffffffff" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="@string/ojnhm"
            android:id="@+id/textView48"
            android:textColor="#ffffffff" />

        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="numberDecimal"
            android:ems="10"
            android:id="@+id/editText2"
            android:textColor="#FFFFFFFF"
            android:numeric="integer|decimal" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView52"
            android:layout_centerHorizontal="true"
            android:textColor="#ffffffff"
            android:text="@string/gbuikh" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="@string/kjnhn"
            android:id="@+id/textView53"
            android:layout_below="@+id/textView52"
            android:layout_centerHorizontal="true"
            android:textColor="#ffffffff" />

        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/iklojniloj"
            android:id="@+id/button8"
            android:layout_below="@+id/textView53"
            android:layout_centerHorizontal="true"
            android:layout_gravity="center_horizontal" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Choose a time zone:"
            android:id="@+id/textView15"
            android:textColor="#ffffffff" />

        <Spinner
            android:layout_width="251dp"
            android:layout_height="wrap_content"
            android:id="@+id/spinner3"
            android:layout_gravity="right"
            android:background="@drawable/btnsighup_new"
            android:entries="@array/time_zone"
            android:prompt="@string/time_zone_prompt"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="@string/lkjljlkj"
            android:id="@+id/textView54"
            android:textColor="#ffffffff"
            android:layout_below="@+id/button8"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <Spinner
            android:layout_width="250dp"
            android:layout_height="wrap_content"
            android:id="@+id/spinner"
            android:background="@drawable/btnsighup_new"
            android:layout_below="@+id/button8"
            android:layout_toRightOf="@+id/editText10"
            android:layout_toEndOf="@+id/editText10"
            android:spinnerMode="dropdown"
            android:prompt="@string/I_am_Quitting_prompt"
            android:entries="@array/I_am_Quitting"
            android:clickable="true"
            android:layout_gravity="right" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="@string/kjnkjn"
            android:id="@+id/textView55"
            android:textColor="#ffffffff"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <Spinner
            android:layout_width="251dp"
            android:layout_height="wrap_content"
            android:id="@+id/spinner2"
            android:layout_alignTop="@+id/textView55"
            android:layout_alignRight="@+id/spinner"
            android:layout_alignEnd="@+id/spinner"
            android:background="@drawable/btnsighup_new"
            android:layout_toRightOf="@+id/editText10"
            android:layout_toEndOf="@+id/editText10"
            android:clickable="true"
            android:prompt="@string/I_am_Quitting_For_prompt"
            android:entries="@array/I_am_Quitting_For"
            android:layout_gravity="right" />

        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:hint="@string/ljlkjn"
            android:textColorHint="#FFFFFFFF"
            android:ems="10"
            android:id="@+id/editText4"
            android:textColor="#FFFFFFFF" />

        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="phone"
            android:ems="10"
            android:id="@+id/editText9"
            android:hint="@string/klljk"
            android:textColorHint="#FFFFFFFF"
            android:textColor="#FFFFFFFF" />

        <Button
            android:layout_width="405dp"
            android:layout_height="fill_parent"
            android:text="@string/ilojilojjnh"
            android:id="@+id/button10"
            android:layout_marginBottom="122dp"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:gravity="center"
            />
    </LinearLayout>
</ScrollView>


</RelativeLayout>
